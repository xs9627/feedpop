{
  "short_name": "Reader",
  "name": "Reader React Extension",
  "manifest_version": 2,
  "permissions": ["storage", "alarms", "<all_urls>"],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "React Ext"
  },
  "icons": { 
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "version": "1.0"
}
