{
  "short_name": "FeedPop",
  "name": "FeedPop: Simple RSS feed reader",
  "manifest_version": 2,
  "permissions": ["storage", "unlimitedStorage", "alarms", "<all_urls>"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "FeedPop"
  },
  "icons": { 
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "version": "1.0"
}
